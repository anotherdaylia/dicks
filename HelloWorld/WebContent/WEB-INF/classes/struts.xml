<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<constant name="struts.devMode" value="true" />
   <package name="helloworld" extends="struts-default">
     <action name="index">
            <result >/index.jsp</result>
      </action>
      <action name="hello" 
            class="com.dicks.engine.HelloWorldAction" 
            method="execute">
            <result name="success">/HelloWorld.jsp</result>
      </action>
      <action name="test" class="com.dicks.engine.SmallTestWeb" method="smallTest">
          <result name="smallTest">/SmallTest.jsp</result>
      </action>
      <action name="createrule" class="com.dicks.engine.CreateNewRuleWeb" method="createrule">
          <result name="createnewrule">/CreateNewRulesResulte.jsp</result>
      </action>
      <action name="gotocreate" class="com.dicks.engine.CreateNewRuleWeb" method="gotocreate">
          <result name="gotocreatenewrule">/CreateNewRules.jsp</result>
      </action>
   </package>
</struts>