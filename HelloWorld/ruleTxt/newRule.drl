rule  "10"
    salience 1
    when
        $o : Order()
        $i : Product( ((length < 1)fsd( length < 2)fsd( length < 3)fsd( length < 4))&& (( sku.equals("aa")))&& (flag.equals("TH-A,ST-A,SP-A")))
