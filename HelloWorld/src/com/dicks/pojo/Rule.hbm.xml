<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
        
<hibernate-mapping>
	
	<class name="com.miqicheng.model.Rule" table="rule_table">
		<id name="ruleID" column="rule_id" type="string">
		</id>
			
		<property name="ruleName" column="rule_name" type="string"></property>
		<property name="ruleDRL" column="rule_drl" type="string"></property>
		<property name="ruleDescr" column="rule_descr" type="string"></property>
		<property name="priority" column="priority" type="int"></property>
	</class>
</hibernate-mapping>