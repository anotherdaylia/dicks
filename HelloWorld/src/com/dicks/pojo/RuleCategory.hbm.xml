<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
        
<hibernate-mapping>
	
	<class name="com.dicks.pojo.RuleCategory" table="rule_cate">
	 <composite-id>
			<key-property name="categoryID" column="category_id" type="int"></key-property>
			<key-property name="ruleID" column="rule_id" type="int"></key-property>		
		</composite-id>  
		

		
		<property name="isProductRelated" column="is_product_related" type="boolean"></property>
		
		<many-to-one name="rule"  class="com.dicks.pojo.Rule" column="rule_id"  insert="false" update="false"></many-to-one>
	</class>
</hibernate-mapping>