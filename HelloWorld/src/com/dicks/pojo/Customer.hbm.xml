<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
        
<hibernate-mapping>
	<class name="com.dicks.pojo.Customer" table="customer">
		<id name="customerID" column="cust_id" type="int">
			<generator class="increment"></generator>
		</id>
			
		<property name="password" column="password" type="string"></property>
		<property name="address" column="address" type="string"></property>
		<property name="zip" column="zip" type="string"></property>
		<property name="tel" column="tel" type="string"></property>
		<property name="sex" column="sex" type="character"></property>
		<property name="customerName" column="cust_name" type="string"></property>
	</class>
</hibernate-mapping>