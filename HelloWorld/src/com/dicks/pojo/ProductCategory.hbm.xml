<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
        
<hibernate-mapping>
	
	<class name="com.dicks.pojo.ProductCategory" table="prod_cate">
		<composite-id>
			<key-property name="prodCategoryID" column="cate_prod_id" type="int"></key-property>
			<key-property name="prodID" column="prod_id" type="int"></key-property>		
		</composite-id> 
			
			
		<property name="CategoryName" column="cate_name" type="string"></property>
		<property name="CategoryDescr" column="cate_descr" type="string"></property>
	</class>
</hibernate-mapping>