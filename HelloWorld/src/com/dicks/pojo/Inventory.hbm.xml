<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
        
<hibernate-mapping>
	
	<class name="com.dicks.pojo.Inventory" table="inventory">
		<composite-id>
			<key-property name="productID" column="prod_id" type="int"></key-property>
			<key-property name="storeID" column="store_id" type="int"></key-property>		
		</composite-id> 
			
		<property name="retailPrice" column="retail_price" type="int"></property>
		<property name="storage" column="inventory" type="int"></property>
		<property name="saftyStock" column="safety_stock" type="int"></property>
	</class>
</hibernate-mapping>