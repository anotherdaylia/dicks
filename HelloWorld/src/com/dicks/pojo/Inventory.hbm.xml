<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
        
<hibernate-mapping>
	
	<class name="com.miqicheng.model.Inventory" table="inventory_table">
		<composite-id>
			<key-property name="productID" column="prod_id" type="string"></key-property>
			<key-property name="storeID" column="store_id" type="string"></key-property>		
		</composite-id> 
			
		<property name="retailPrice" column="price" type="int"></property>
		<property name="storage" column="storage" type="int"></property>
		<property name="saftyStock" column="safty_stock" type="int"></property>
	</class>
</hibernate-mapping>