<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
        
<hibernate-mapping>
	
	<class name="com.miqicheng.model.Person" table="person">
		<id name="storeID" column="store_id" type="string">
		</id>
			
		<property name="address" column="address" type="string"></property>
		<property name="ruleID" column="rule_id" type="string"></property>
		<property name="storeName" column="store_name" type="string"></property>
	</class>
</hibernate-mapping>